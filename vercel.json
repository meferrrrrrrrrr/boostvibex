{
  "rewrites": [
    {
      "has": [
        { "type": "host", "value": "boostvibex.com" }
      ],
      "source": "/(.*)",
      "destination": "/under-construction.html"
    },
    {
      "has": [
        { "type": "host", "value": "www.boostvibex.com" }
      ],
      "source": "/(.*)",
      "destination": "/under-construction.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/privacy",
      "destination": "/public/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/public/terms.html"
    },
    {
      "source": "/contact",
      "destination": "/public/contact.html"
    },
    {
      "source": "/under-construction",
      "destination": "/public/under-construction.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}